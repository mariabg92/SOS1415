<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Prueba</servlet-name>
		<servlet-class>es.us.sos.PruebaServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Prueba</servlet-name>
		<url-pattern>/prueba</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Augusto</servlet-name>
		<servlet-class>es.us.sos.AugustoServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Augusto</servlet-name>
		<url-pattern>/saludo/manu</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Augusto1</servlet-name>
		<servlet-class>es.us.sos.AugustoServlet1</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Augusto1</servlet-name>
		<url-pattern>/saludo/mariajose</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
</web-app>
